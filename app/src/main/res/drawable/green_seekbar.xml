<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background"
        >
        <shape
            android:width="300dp"
            android:height="20dp">
            <solid android:color="#FFEDEAE4"/>

            <corners android:radius="10dp"/>


        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape
                android:width="300dp"
                android:height="20dp">

                <corners android:radius="10dp"/>

                <solid android:color="#FF808082"/>


            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape
                android:width="300dp"
                android:height="20dp">

                <corners android:radius="10dp"/>

                <solid android:color="@color/theme_color_green"/>


            </shape>
        </clip>
    </item>

</layer-list>